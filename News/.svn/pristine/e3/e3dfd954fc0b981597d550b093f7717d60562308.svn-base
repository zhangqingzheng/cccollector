<ui:composition xmlns="http://www.w3.org/1999/xhtml" 
				xmlns:ui="http://java.sun.com/jsf/facelets"
			xmlns:h="http://java.sun.com/jsf/html" 
			xmlns:f="http://java.sun.com/jsf/core"
			xmlns:c="http://java.sun.com/jsp/jstl/core" 
			xmlns:p="http://primefaces.org/ui">
	<ui:define name="head">
		<f:metadata>
			<f:viewParam name="contentId" value="#{tempContentHtmlBean.modelId1}" />
			<f:viewParam name="contentType" value="#{tempContentHtmlBean.modelId2}" />
			<f:viewParam name="siteVersionId" value="#{tempContentHtmlBean.modelId3}" />
			<f:viewParam name="templateVersionId" value="#{tempContentHtmlBean.modelId4}" />
		</f:metadata>
	</ui:define>
	<h:outputText value="#{tempContentHtmlBean.html}"  escape="false"/>
</ui:composition>