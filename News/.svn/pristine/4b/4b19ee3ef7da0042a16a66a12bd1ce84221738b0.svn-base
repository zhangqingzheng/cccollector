package com.cccollector.news.templatemodel;

import java.io.File;

/**
 * 页面信息，栏目页面以及文章页面的基类
 */
public class TemplateBaseContent {

	/**
	 * 地址栏标题
	 */
	private String _title;

	public String getTitle() {
		return _title;
	}

	public void setTitle(String title) {
		_title = title;
	}

	/**
	 * 首页栏目
	 */
	private TemplateColumn _homePageColumn;

	public TemplateColumn getHomePageColumn() {
		return _homePageColumn;
	}

	public void setHomePageColumn(TemplateColumn homePageColumn) {
		_homePageColumn = homePageColumn;
	}

	/**
	 * 精选阅读栏目
	 */
	private TemplateColumn _selectionColumn;

	public TemplateColumn getSelectionColumn() {
		return _selectionColumn;
	}

	public void setSelectionColumn(TemplateColumn selectionColumn) {
		_selectionColumn = selectionColumn;
	}

	/**
	 * 文藏合作栏目
	 */
	private TemplateColumn _cooperationColumn;

	public TemplateColumn getCooperationColumn() {
		return _cooperationColumn;
	}

	public void setCooperationColumn(TemplateColumn cooperationColumn) {
		_cooperationColumn = cooperationColumn;
	}

	/**
	 * 当前栏目
	 */
	private TemplateColumn _currentColumn;

	public TemplateColumn getCurrentColumn() {
		return _currentColumn;
	}

	public void setCurrentColumn(TemplateColumn currentColumn) {
		_currentColumn = currentColumn;
	}

	/**
	 * 当前选择的二级栏目
	 */
	private int _currentSecondColumnId;

	public int getCurrentSecondColumnId() {
		return _currentSecondColumnId;
	}

	public void setCurrentSecondColumnId(int currentSecondColumnId) {
		_currentSecondColumnId = currentSecondColumnId;
	}

	private File _templateFile;

	/**
	 * 模板文件存储路径
	 */
	public File getTemplateFile() {
		return _templateFile;
	}

	public void setTemplateFile(File templateFile) {
		this._templateFile = templateFile;
	}

	private File _tomcatFolder;

	public File getTomcatFolder() {
		return _tomcatFolder;
	}

	public void setTomcatFolder(File tomcatFolder) {
		this._tomcatFolder = tomcatFolder;
	}

	private File _nginxFolder;

	public File getNginxFolder() {
		return _nginxFolder;
	}

	public void setNginxFolder(File nginxFolder) {
		this._nginxFolder = nginxFolder;
	}

	private File _testFolder;

	public File getTestFolder() {
		return _testFolder;
	}

	public void setTestFolder(File testFolder) {
		this._testFolder = testFolder;
	}

	private String _staticFilePath;

	public String getStaticFilePath() {
		return _staticFilePath;
	}

	public void setStaticFilePath(String staticFilePath) {
		this._staticFilePath = staticFilePath;
	}

	public int _siteVersionId;

	/**
	 * 站点版本
	 */
	public int getSiteVersionId() {
		return _siteVersionId;
	}

	public void setSiteVersionId(int siteVersionId) {
		this._siteVersionId = siteVersionId;
	}

	private Integer _templateVersionId;

	/**
	 * 模板版本ID
	 */
	public Integer getTemplateVersionId() {
		return _templateVersionId;
	}

	public void setTemplateVersionId(Integer templateVersionId) {
		_templateVersionId = templateVersionId;
	}
}
