package com.cccollector.news.templatemodel;

import java.util.ArrayList;
import java.util.List;

public class TemplateColumnContent extends TemplateBaseContent {

	private List<TemplateRecommend> _templateRecommends = new ArrayList<TemplateRecommend>();

	/**
	 * 关联的模板推荐列表
	 */
	public List<TemplateRecommend> getTemplateRecommends() {
		return _templateRecommends;
	}

	public void setTemplateRecommends(List<TemplateRecommend> templateRecommends) {
		_templateRecommends = templateRecommends;
	}

	private List<TemplateColumn> _thirdLevelColumns = new ArrayList<TemplateColumn>();

	/**
	 * 三级栏目列表
	 */
	public List<TemplateColumn> getThirdLevelColumns() {
		return _thirdLevelColumns;
	}

	public void setThirdLevelColumns(List<TemplateColumn> thirdLevelColumns) {
		_thirdLevelColumns = thirdLevelColumns;
	}

	private List<TemplateArticle> _templateArticles;

	/**
	 * 分发列表
	 */
	public List<TemplateArticle> getTemplateArticles() {
		return _templateArticles;
	}

	public void setTemplateArticles(List<TemplateArticle> templateArticles) {
		this._templateArticles = templateArticles;
	}

	private boolean _hasMoreTemplateArticles;

	/**
	 * 是否有更多的分发
	 */
	public boolean getHasMoreTemplateArticles() {
		return _hasMoreTemplateArticles;
	}

	public void setHasMoreTemplateArticles(boolean hasMoreTemplateArticles) {
		this._hasMoreTemplateArticles = hasMoreTemplateArticles;
	}

	private String _staticSourceUrl;

	/**
	 * 回源地址
	 */
	public String getStaticSourceUrl() {
		return _staticSourceUrl;
	}

	public void setStaticSourceUrl(String staticSourceUrl) {
		this._staticSourceUrl = staticSourceUrl;
	}

}
