package com.cccollector.news.templatemodel;

import java.io.Serializable;

public class TemplateCategoryTag implements Serializable {

	private static final long serialVersionUID = -746821744266046269L;
	
	private Integer _categoryTagId;

	/**
	 * 分类标签ID
	 */
	public Integer getCategoryTagId() {
		return _categoryTagId;
	}

	public void setCategoryTagId(Integer categoryTagId) {
		_categoryTagId = categoryTagId;
	}

	private String _name;

	/**
	 * 名称
	 */
	public String getName() {
		return _name;
	}

	public void setName(String name) {
		_name = name;
	}

	private String _link;
	
	/**
	 * 链接
	 */	
	public String getLink() {
		return _link;
	}

	public void setLink(String link) {
		_link = link;
	}
}